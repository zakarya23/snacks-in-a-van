{{> formOpen }}        
		<h1 class="formHeading">
            Vendor Login
        </h1>
		<form method="POST" action="/vendor">
			<div class="formLine">
				Van Name <input class="formField" type="text" name="name">
			</div>
			<div class="formLine">
				Password <input class="formField" type="password" name="password">
			</div>
			
			<span class="dialogueButtonLine">
				<a href= "/vendor/home"></a>
				<button class="loginButton" type="submit">Continue</button>
			</span>
		</form>
	</div>

</div>